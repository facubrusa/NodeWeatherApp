{
  "name": "weatherapp",
  "version": "0.1.0",
  "description": "Weather app made in NodeJS and using Weather API",
  "main": "server.js",
  "scripts": {
    "dev": "NODE_ENV=development nodemon ./src/server.js",
    "start": "NODE_ENV=production node ./src/server.js",
    "lint": "eslint",
    "test": "NODE_ENV=test jest --verbose --silent --maxWorkers=1",
    "test:watch": "npm run test -- --watch"
  },
  "keywords": [
    "weather",
    "nodejs",
    "telecom"
  ],
  "author": "Facundo Brusa",
  "license": "ISC",
  "dependencies": {
    "@hapi/boom": "^10.0.0",
    "dotenv": "^16.0.1",
    "ejs": "^3.1.8",
    "express": "^4.18.1",
    "node-fetch": "^2.6.7",
    "path": "^0.12.7",
    "request": "^2.88.2",
    "request-ip": "^3.3.0"
  },
  "devDependencies": {
    "eslint": "^8.20.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "jest": "^28.1.3",
    "nodemon": "^2.0.19",
    "prettier": "^2.7.1",
    "supertest": "^6.2.4"
  },
  "jest": {
    "testEnvironment": "node"
  }
}
